!URxvt*.transparent: true
!URxvt*.shading: 1
URxvt.scrollBar: false
URxvt.geometry: 180x60
!URxvt.geometry: 85x27
URxvt.depth: 32

! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

URxvt.keysym.Home: \033[1~
URxvt.keysym.End: \033[4~
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~

URxvt.borderless: 1
URxvt.saveLines: 5000
URxvt.termName: rxvt-256color
URxvt.urlLauncher: firefox
URxvt.matcher.button: 1
URxvt.cursorBlink: 1

URxvt.iso14755: true
URxvt.iso14755_52: false

URxvt.letterSpace: -1

URxvt.internalBorder: 4

URxvt.urgentOnBell: True

URxvt*font: xft:Hack:medium:size=9
URxvt*boldFont: xft:Hack:bold:size=9
URxvt*italicFont: xft:Hack:medium:italic:size=9
URxvt*bolditalicFont: xft:Hack:bold:italic:size=9

URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~

